        <%= image_tag("card-back.png", :size => "100x150") %>
      </div>
    </div>
    <%= form_for @round do |f| %>
    <div class="ante-div">
  	<%= f.label :ante_amount %>
  	<%= f.text_field :ante_amount %>
    </div>

    <%= f.hidden_field :user_id, :value => session[:user_id] %>
    <%= f.hidden_field :status, :value => "active" %>
  	<%= f.submit ["Deal!"] %>
  <%end%>
  </body>
