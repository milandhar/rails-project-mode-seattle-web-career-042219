<h1><%= @user.username %> Has Been Logged Out! </h1>


<div class="login-div">

<%= form_tag '/login' do %>
<%= label_tag :username %>
<%= text_field_tag :username %>
<%= submit_tag "Log In" %>
<% end %>

<a id = "new-profile-link" href="/users/new">Create New Profile</a>

</div>


<div class="card-bottom">
  <%= image_tag("card-back.png", :size => "150x225") %>
</div>
