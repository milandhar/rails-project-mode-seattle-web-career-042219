<div class="nav-box">
      <nav id = "navbar">
        <h4>Logged in as <%= User.find(session[:user_id]).username %></h4>
        <ul>
          <li><%= link_to "Go to Users List", users_path %></li>
          <li><%= link_to "Edit Profile", edit_user_path(User.find(session[:user_id])) %></li>
          <li><%= link_to "Log Out", logout_path %></li>
        </ul>
      </nav>
  </div>

<h1>All Players:</h1>
<div class="body">
<div class="user_list">
<% i = 1 %>
<% @ordered_users.each do |user| %>
  <h2><%= i%>. <%= link_to user.username, user  %></h2>
  <ul>
    <li class = "index_bullets">Name: <%= user.name %> </li>
    <li class = "index_bullets">Points: <%= user.points %> </li>
  </ul>
  <% i+= 1 %>
<% end %>
</div>
<div class="index_images">
  <div class="left_cards">
    <%= image_tag("card-images/QH.png", :size => "150x225") %>
    <%= image_tag("card-back.png", :size => "150x225") %>
    <%= image_tag("card-images/KS.png", :size => "150x225") %>
    <%= image_tag("card-back.png", :size => "150x225") %>
    <%= image_tag("card-images/KS.png", :size => "150x225") %>
  </div>
  <div class="right_cards">
    <%= image_tag("card-back.png", :size => "150x225") %>
    <%= image_tag("card-images/JD.png", :size => "150x225") %>
    <%= image_tag("card-back.png", :size => "150x225") %>
    <%= image_tag("card-images/JD.png", :size => "150x225") %>
    <%= image_tag("card-back.png", :size => "150x225") %>
  </div>

</div>

</div>
